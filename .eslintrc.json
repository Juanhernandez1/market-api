{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier", "babel"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "camelcase": "off"
  }
}
